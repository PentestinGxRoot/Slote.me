{% trans_default_domain 'FOSUserBundle' %}
<style type="text/css">
body, .input{
	color:black;
	text-shadow:black;
}
</style>
<div class="container" style="margin-top:20%">
	<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register login">
		<div class="form-group">
			<fieldset class="col-10">
				<legend class="legend">Register</legend>
				{{ form_widget(form) }}
				<div>
				</div>
				 <button type="submit" class="submit" name="_submit"  value="{{ 'registration.submit'|trans }}"><i class="fa fa-long-arrow-right"></i></button>
			</div>
		</fieldset>
	  <div class="feedback">
        login successful <br />
        redirecting...
      </div>
	</form>
</div>