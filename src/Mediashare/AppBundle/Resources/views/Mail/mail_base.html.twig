<!DOCTYPE html>
<html>
<head>
    <title>Cloture Session</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style type="text/css">
        /* Fonts and Content */
        body, td {
            font-family: 'Helvetica Neue', Arial, Helvetica, Geneva, sans-serif;
            font-size: 14px;
        }

        body {
            background-color: #F2F0F0;
            margin: 0;
            padding: 0;
            -webkit-text-size-adjust: none;
            -ms-text-size-adjust: none;
        }

        h2 {
            padding-top: 12px; /* ne fonctionnera pas sous Outlook 2007+ */
            color: #0E7693;
            font-size: 22px;
        }

        .tableResume {
            width: 75%;
            border-bottom: #ccc solid 1px;
            background: #FFF;
            font-size: 0.850em;
            text-align: center;
            /*margin-left: 3%;*/
        }

        .tableResume tr {
            border-bottom: #ccc dotted 1px;
        }

        .tableResume th {
            padding: 0.5em 1em;
            background: #3f444c;
            color: #FFF;
        }

        .borde {
            border-top: #EEE solid 1px;
            border-left: #EEE solid 1px;
            border-right: #EEE solid 1px;
        }

        .tableObject {
            width: 75%;
            border-bottom: #ccc solid 1px;
            background: #FFF;
            font-size: 0.850em;
            text-align: center;
            margin-left: 3%;
        }

        .tableObject tr {
            border-bottom: #ccc dotted 1px;
        }

        .tableObject th {
            padding: 0.5em 1em;
            background: #3f444c;
            color: #FFF;
        }

        .inProgressOk {
            background: #d5d8dd;
            display: block;
            color: #FFF;
        }

        .inProgress {
            background: #50a98d;
            display: block;
            color: #FFF;
        }

        .closed {
            background: #e64014;
            display: block;
            color: #FFF;
        }

        .resume {
            background: #3F444C;
            display: block;
            color: #FFF;
        }
    </style>

    <style type="text/css"></style>
    <meta name="chromesniffer" id="chromesniffer_meta" content="{}">
    <script type="text/javascript" src="chrome-extension://homgcnaoacgigpkkljjjekpignblkeae/detector.js"></script>
</head>

<body style="margin:0px; padding:0px; -webkit-text-size-adjust:none;">
{% block body %}

    <table width="100%" cellpadding="0" cellspacing="0" border="0" style="background-color:rgb(42, 55, 78)">
        <tbody>
        <tr>
            <td align="center" bgcolor="#F2F0F0">
                <table cellpadding="0" cellspacing="0" border="0">
                    <tbody>
                    <tr>
                        <td class="w640" width="640" height="10"></td>
                    </tr>

                    <tr>
                        <td align="center" class="w640" width="640" height="20"><a
                                    style="color:#255D5C; font-size:12px;"
                                    href="{{ base }}"><span
                                        style="color:#255D5C; font-size:12px;">Allez sur le site "Nom du site internet"</span></a>
                        </td>
                    </tr>
                    <tr>
                        <td class="w640" width="640" height="10"></td>
                    </tr>


                    <!-- entete -->
                    <tr class="pagetoplogo">
                        <td class="w640" width="640">
                            <table class="w640 borde" width="640" cellpadding="0" cellspacing="0" border="0"
                                   bgcolor="#EEE">
                                <tbody>
                                <tr>
                                    <td class="w30" width="30"></td>
                                    <td class="w580" width="580" valign="middle" align="left">
                                        <div class="pagetoplogo-content" style="height: 105px">
                                            <img class="w300"
                                                 style="text-decoration: none; display: block; color:#476688; font-size:30px; padding-top: 25px; padding-left: 25%"
                                                 src="{{ base }}images/logo-hydrex.png"
                                                 alt="Mon Logo" width="300" height="50">
                                        </div>
                                    </td>
                                    <td class="w30" width="30"></td>
                                </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>

                    <!-- separateur horizontal -->
                    <tr>
                        <td class="w640" width="640" height="1" bgcolor="#d7d6d6"></td>
                    </tr>
                    {% block contenu %}
                        <!-- contenu -->
                    {% endblock %}
                    <!--  separateur horizontal de 15px de  haut-->
                    <tr>
                        <td class="w640" width="640" height="15" bgcolor="#ffffff"></td>
                    </tr>


                    <!--  separateur horizontal de 15px de  haut-->
                    <tr>
                        <td class="w640" width="640" height="15" bgcolor="#ffffff"></td>
                    </tr>
                    {% block footer %}
                        <!-- pied de page -->
                        <tr class="pagebottom">
                            <td class="w640" width="640">
                                <table class="w640" width="640" cellpadding="0" cellspacing="0" border="0"
                                       bgcolor="#c7c7c7">
                                    <tbody>
                                    <tr>
                                        <td colspan="5" height="10"></td>
                                    </tr>
                                    <tr>
                                        <td class="w30" width="30"></td>
                                        <td class="w580" width="580" valign="top">
                                            <p align="right" class="pagebottom-content-left">
                                                <a style="color:#255D5C;"
                                                   href="https://www.facebook.com/Serco-Point-Web-105647946138280/"><span
                                                            style="color:#255D5C;">Serco Point Web</span></a>
                                            </p>
                                        </td>

                                        <td class="w30" width="30"></td>
                                    </tr>
                                    <tr>
                                        <td colspan="5" height="10"></td>
                                    </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td class="w640" width="640" height="60"></td>
                        </tr>
                    {% endblock %}
                    </tbody>
                </table>
            </td>
        </tr>
        </tbody>
    </table>


{% endblock %}
</body>
</html>